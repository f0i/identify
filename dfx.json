{
  "networks": {
    "ic-odoc": {
      "providers": ["https://icp0.io"],
      "type": "persistent"
    }
  },
  "canisters": {
    "backend": {
      "main": "src/backend/main.mo",
      "type": "motoko"
    },
    "frontend": {
      "dependencies": ["backend"],
      "frontend": {
        "entrypoint": "out/frontend/index.html"
      },
      "source": ["out/frontend"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "output_env_file": ".env",
  "version": 1
}
